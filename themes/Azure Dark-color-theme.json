        "storage"
      ],
      "settings": {
        "foreground": "#88DDFF"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.other",
      ],
      "settings": {
        "foreground": "#FF66BB"
      }
    },
    {
      "name": "Separators like ; or ,",
