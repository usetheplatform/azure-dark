  "tokenColors": [
    {
      "settings": {
        "background": "#0a0e14",
        "foreground": "#b3b1ad"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#626a73"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#91DD64"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": [
        "string.regexp",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#00AEFF"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#b3b1ad"
      }
    },
    {
      "name": "Member Variable",
      "scope": [
        "variable.member"
      ],
      "settings": {
        "foreground": "#f07178"
