      "settings": {
        "foreground": "#FFD14A"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.other",
        "keyword.pipe",
        "keyword.unused"
      ],
      "settings": {
        "foreground": "#FF66BB"
      }
    },
    {
      "name": "Separators like ; or ,",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator"
