{
  "name": "Azure Light",
  "type": "light",
  "colors": {
    "editor.background": "#F7F7F7",
    "editor.foreground": "#000",
    "editor.lineHighlightBackground": "#F0F0F0",
    "editor.selectionBackground": "#BFDBFE",
    "editor.selectionHighlightBackground": "#E0E0E0",
    "panel.background": "#F0F0F0",
    "sideBar.background": "#F0F0F0",
    "editorGroupHeader.tabsBackground": "#F0F0F0",
    "activityBar.background": "#F0F0F0",
    "activityBar.foreground": "#1293D8",
    "editorLineNumber.foreground": "#9DA39A",
    "editorCursor.foreground": "#1293D8",
    "editor.findMatchBackground": "#FFBC5D",
    "editor.findMatchHighlightBackground": "#FFE9A6",
    "statusBar.background": "#DDDDDD",
    "statusBar.foreground": "#474747",
    "statusBar.debuggingBackground": "#FFBC5D",
    "statusBar.debuggingForeground": "#000",
    "statusBar.noFolderBackground": "#7A3E9D",
    "statusBar.noFolderForeground": "#fff",
    "terminal.ansiWhite": "#BBBBBB",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#325CC0",
    "terminal.ansiCyan": "#0083B2",
    "terminal.ansiGreen": "#448C27",
    "terminal.ansiMagenta": "#7A3E9D",
    "terminal.ansiRed": "#AA3731",
    "terminal.ansiYellow": "#CB9000",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightBlack": "#777777",
    "terminal.ansiBrightBlue": "#1293D8",
    "terminal.ansiBrightCyan": "#00AACB",
    "terminal.ansiBrightGreen": "#60CB00",
    "terminal.ansiBrightMagenta": "#E64CE6",
    "terminal.ansiBrightRed": "#F05050",
    "terminal.ansiBrightYellow": "#FFBC5D"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#0a0e14",
        "foreground": "#b3b1ad"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6A737D"
      }
    },
    {
      "name": "Unit",
      "scope": [
        "constant.unit"
      ],
      "settings": {
        "foreground": "#000E16",
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": [
        "string.regexp",
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Member Variable",
      "scope": [
        "variable.member"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Language variable",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "constant.type-constructor"
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Module Declaration",
      "scope": [
        "meta.declaration.module keyword.other"
      ],
      "settings": {
        "foreground": "#859900"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.other",
        "keyword.pipe",
        "keyword.unused",
        "punctuation",
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Separators like ; or ,",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Accessor",
      "scope": [
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Types fixes",
      "scope": [
        "source.java storage.type",
        "source.haskell storage.type",
        "source.c storage.type"
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Inherited class type",
      "scope": [
        "entity.other.inherited-class",
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Lambda arrow",
      "scope": [
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Java primitive variable types",
      "scope": [
        "source.java storage.type.primitive"
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Function name type definition",
      "scope": [
        "meta.function.type-declaration",
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "Function name toplevel",
      "scope": [
        "entity.name.function.top_level",
        "source.js entity.name.function",
        "souce.ts entity.name.function",
        "source.tsx entity.name.function",
        "source.jsx entity.name.function",
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "Function arguments",
      "scope": [
        "variable.parameter",
        "meta.parameter"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "variable.function",
        "variable.annotation",
        "meta.function-call.generic",
        "support.function.go"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Module in Type definition",
      "scope": [
        "meta.function.type-declaration meta.module.name"
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function",
        "support.macro"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Imports and packages",
      "scope": [
        "entity.name.import",
        "entity.name.package"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Record fields",
      "scope": [
        "entity.name.record.field"
      ],
      "settings": {
        "foreground": "#000E16",
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Library type",
      "scope": [
        "support.type",
        "source.go storage.type"
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Library class",
      "scope": [
        "support.class",
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "Decorators/annotation",
      "scope": [
        "meta.decorator variable.other",
        "meta.decorator punctuation.decorator",
        "storage.type.annotation"
      ],
      "settings": {
        "foreground": "#b58900"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#CD3131"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#c594c5"
      }
    },
    {
      "name": "Ruby class methods",
      "scope": [
        "source.ruby variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "CSS tag names",
      "scope": [
        "source.css entity.name.tag",
        "source.sass entity.name.tag",
        "source.scss entity.name.tag",
        "source.less entity.name.tag",
        "source.stylus entity.name.tag"
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "CSS browser prefix",
      "scope": [
        "source.css support.type",
        "source.sass support.type",
        "source.scss support.type",
        "source.less support.type",
        "source.stylus support.type"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#000E16"
      }
    },
    {
      "name": "Search Results Nums",
      "scope": [
        "constant.numeric.line-number.find-in-files - match"
      ],
      "settings": {
        "foreground": "#000E16"
      }
    },
    {
      "name": "Search Results Match Nums",
      "scope": [
        "constant.numeric.line-number.match"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Search Results Lines",
      "scope": [
        "entity.name.filename.find-in-files"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "scope": [
        "message.error"
      ],
      "settings": {
        "foreground": "#CD3131"
      }
    },
    {
      "name": "Markup heading",
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Markup links",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f07178"
      }
    },
    {
      "name": "Markup Bold/italic",
      "scope": [
        "markup.italic markup.bold",
        "markup.bold markup.italic"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Markup Code",
      "scope": [
        "markup.raw"
      ],
      "settings": {
        "background": "#b3b1ad05"
      }
    },
    {
      "name": "Markup Code Inline",
      "scope": [
        "markup.raw.inline"
      ],
      "settings": {
        "background": "#b3b1ad0f"
      }
    },
    {
      "name": "Markdown Separator",
      "scope": [
        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "background": "#b3b1ad0f",
        "foreground": "#000E16"
      }
    },
    {
      "name": "Markup Blockquote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#2aa198",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup List Bullet",
      "scope": [
        "markup.list punctuation.definition.list.begin"
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "Markup added",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#2aa198"
      }
    },
    {
      "name": "Markup modified",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#1293D8"
      }
    },
    {
      "name": "Markup removed",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#d96c75"
      }
    },
    {
      "name": "Markup Strike",
      "scope": [
        "markup.strike"
      ],
      "settings": {
        "foreground": "#e6b673"
      }
    },
    {
      "name": "Markup Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "background": "#b3b1ad0f",
        "foreground": "#1293D8"
      }
    },
    {
      "name": "Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw"
      ],
      "settings": {
        "foreground": "#FF66BB"
      }
    },
    {
      "name": "Markdown - Line Break",
      "scope": [
        "text.html.markdown meta.dummy.line-break"
      ],
      "settings": {
        "background": "#626a73",
        "foreground": "#626a73"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": [
        "punctuation.definition.markdown"
      ],
      "settings": {
        "background": "#b3b1ad",
        "foreground": "#626a73"
      },
    },
  ]
}
